{
  "name": "love_hub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:pages": "BASE_URL=/${npm_package_name}/ vite build",
    "deploy": "npm run build:pages && gh-pages -d dist -b gh-pages --nojekyll",
    "deploy:safe": "git add -A && (git commit -m 'chore: deploy' || git commit -m 'chore: deploy' --allow-empty) && git push -u origin main && npm run deploy",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/charts": "^2.6.7",
    "@ant-design/icons": "^6.1.0",
    "@supabase/supabase-js": "^2.78.0",
    "antd": "^6.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "vite": "^4.5.0"
  }
}
